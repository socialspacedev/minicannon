{
  "name": "minimalism",
  "version": "0.1.05",
  "description": "I beg you God, make sure that it is the last!",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "go!": "run-s clean css eleventy",
    "clean": "rimraf public",
    "css": "npx tailwindcss -i src/_11ty/_tailwindCSS/raw-website.css -o public/css/style.css --minify",
    "eleventy": "npx @11ty/eleventy",
    "dev:css": "npx tailwindcss -i src/_11ty/_tailwindCSS/raw-website.css -o public/css/style.css --watch --minify",
    "dev:11ty": "eleventy --serve --watch"
  },
  "keywords": [
    "Website",
    "Blog",
    "11ty",
    "Tailwindcss",
    "SEO",
    "CloudCannon"
  ],
  "author": "Andrew Long",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@bookshop/browser": "3.8.2",
    "@bookshop/eleventy-engine": "3.8.2",
    "@bookshop/generate": "3.8.2",
    "@tailwindcss/typography": "^0.5.2",
    "html-minifier": "^4.0.0",
    "luxon": "^2.3.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@bookshop/eleventy-bookshop": "^3.8.1",
    "eleventy": "^1.0.6",
    "eleventy-plugin-time-to-read": "^1.3.0",
    "pagefind": "^1.0.4"
  }
}
