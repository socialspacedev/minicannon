{
  "name": "minicannon",
  "version": "1.0.0",
  "description": "Make this build please",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:*",
    "go!": "run-s clean css eleventy",
    "clean": "rimraf public",
    "css": "npx tailwindcss -i src/_css/site.css -o public/css/style.css --minify",
    "eleventy": "npx @11ty/eleventy",
    "dev:css": "npx tailwindcss -i src/_css/site.css -o public/css/style.css --watch --minify",
    "dev:11ty": "eleventy --serve --watch"
  },
  "keywords": [
    "Website",
    "Blog",
    "11ty",
    "Tailwindcss",
    "SEO",
    "CloudCannon"
  ],
  "author": "Andrew Long",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@divriots/jampack": "^0.15.0",
    "@tailwindcss/typography": "^0.5.2",
    "html-minifier": "^4.0.0",
    "luxon": "^2.3.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^3.1.8",
    "eleventy": "^1.0.6",
    "eleventy-plugin-time-to-read": "^1.3.0",
    "eleventy-plugin-youtube-embed": "^1.10.2",
    "pagefind": "^1.0.4"
  }
}
