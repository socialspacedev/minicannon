    <h3>Latest Post</h3>
  <a href="{{ latestPost.url }}">{{ latestPost.data.title }}</a>
  </div>
{%- endif -%}
    </main>
    {% include "footer.liquid" %}
  </body>
<!-- End Body -->
</html>